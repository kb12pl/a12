{
  "expo": {
    "name": "a12",
    "slug": "a12",
    "version": "15.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/kb12.png",
    "scheme": "a12",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/kb12.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/kb12.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.kb12pl.a12",
      "versionCode": 17
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/kb12.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "5467eeb1-228f-40ea-a204-115156d0d839"
      }
    }
  }
}
